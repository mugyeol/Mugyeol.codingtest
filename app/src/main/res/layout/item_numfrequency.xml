<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:padding="15dp"
    android:layout_height="wrap_content">



    <!--//회차-->
    <TextView

        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        android:gravity="center"
        android:textSize="18sp"
        android:id="@+id/numfrequencyNum"
        android:layout_width="50dp"
        android:layout_height="wrap_content" />

    <View
        android:layout_marginLeft="60dp"
        app:layout_constraintTop_toTopOf="@id/numfrequencyNum"
        app:layout_constraintBottom_toBottomOf="@id/numfrequencyNum"
        app:layout_constraintLeft_toLeftOf="parent"
        android:background="@drawable/cornerroundgray"
        android:layout_height="13dp"
        android:layout_width="200dp"

        android:id="@+id/numfrequencyView"

        />
    <TextView
        android:layout_marginLeft="20dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toRightOf="@id/numfrequencyView"
        android:gravity="center"
        android:textSize="18sp"
        android:text="sdf"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />
</androidx.constraintlayout.widget.ConstraintLayout>





